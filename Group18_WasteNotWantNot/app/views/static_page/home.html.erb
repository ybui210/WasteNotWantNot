    <div class="center">
      <h1>Save your food with our app</h1>

      <%= link_to "Sign up now!", new_user_path, class: "btn btn-lg btn-primary" %>
    </div>
  <!--  <%= debug(params) if Rails.env.development? %>-->
  </div>
</body>
